{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/health",
      "dest": "src/index.js"
    },
    {
      "src": "/mobile/(.*)",
      "dest": "/public/mobile/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/public/admin/$1"
    },
    {
      "src": "/((.*\\.html|.*\\.js|.*\\.css|.*\\.json|.*\\.manifest))",
      "dest": "/public/$1"
    }
  ]
}
